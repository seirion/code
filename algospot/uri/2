#include <iostream>
#include <cstring>
#include <cstdlib>
#include <cstdio>

using namespace std;

const char CODE[] = {
    ' ',
    '!',
    ' ',
    ' ',
    '$',
    '%',
    ' ',
    ' ',
    '(',
    ')',
    '*'
}

void solve() {
    char buffer[128];
    cin >> buffer;

    size_t size = strlen(buffer);
    for (int i = 0; i < size; i++) {
        if (buffer[i] == '%') {
        }
        else {
            cout << buffer[i];
        }
    }
    cout << endl;
}

int main() {
    int num;
    cin >> num;

    for (int i = 0; i < num; i++) {
        solve();
    }
    return 0;
}
